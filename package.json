{
  "name": "med-queue-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wait-on": "^7.2.0",
    "dotenv": "^16.4.5",
    "mongoose": "^8.19.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "scripts": {
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "build": "npm run install-client && npm run build-client",
    "start": "cd server && npm start",
    "client:build": "cd client && npm run build",
    "server:start": "cd server && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"wait-on http-get://localhost:5002/api/auth/health && npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "dev:all": "concurrently \"npm run server\" \"npm run client\"",
    "dev:start": "node scripts/start-dev.js",
    "create-admin": "cd server && npm run create-admin"
  }
}